passwd:
  users:
    - name: home
      uid: 1014
      primary_group: home
      shell: /bin/false
      groups:
        - dockerhub
        - aivault
  groups:
    - name: home
      gid: 1014
systemd:
  units:
    - name: container-dbus.service
      enabled: true
storage:
  files:
    - path: /var/lib/systemd/linger/home
      mode: 0o444
      contents:
        inline: ''
    - path: /usr/local/etc/nginx/conf.d/home.local.conf
      mode: 0o444
    - path: /usr/local/etc/nginx/conf.d/zigbee.local.conf
      mode: 0o444
    - path: /usr/local/share/homeassistant-run.sh
      mode: 0o555
    - path: /usr/local/share/container-dbus.pp
      mode: 0o444
    - path: /var/mnt/vault/.config/mosquitto/config/mosquitto-passwords
      mode: 0o400
      overwrite: true
      user:
        name: home
      group:
        name: home
    - path: /var/mnt/vault/.config/mosquitto/config/mosquitto.conf
      mode: 0o555
      overwrite: true
      user:
        name: home
      group:
        name: home
    - path: /var/mnt/vault/.config/zigbee2mqtt/configuration.yaml
      mode: 0o750
      overwrite: true
      file: zigbee2mqtt-configuration.yaml
      user:
        name: home
      group:
        name: aivault
    - path: /var/mnt/vault/.config/homeassistant/blueprints/automation/wall-switch.yaml
      mode: 0o770
      overwrite: true
      user:
        name: home
      group:
        name: aivault
    - path: /var/mnt/vault/.config/homeassistant/blueprints/automation/presence.yaml
      mode: 0o770
      overwrite: true
      user:
        name: home
      group:
        name: aivault
    - path: /usr/local/etc/home.env
      mode: 0o400
      user:
        name: home
      group:
        name: home
    - path: /home/home/.config/systemd/user/homeassistant.service
    - path: /home/home/.config/systemd/user/zigbee2mqtt.service
    - path: /home/home/.config/systemd/user/mosquitto.service
  links:
    - path: /home/home/.config/systemd/user/default.target.wants/homeassistant.service
      target: /home/home/.config/systemd/user/homeassistant.service
      hard: false
    - path: /home/home/.config/systemd/user/default.target.wants/zigbee2mqtt.service
      target: /home/home/.config/systemd/user/zigbee2mqtt.service
      hard: false
    - path: /home/home/.config/systemd/user/default.target.wants/mosquitto.service
      target: /home/home/.config/systemd/user/mosquitto.service
      hard: false
  directories:
    - path: /var/mnt/vault/.config/homeassistant
      mode: 0o755
      user:
        name: home
      group:
        name: aivault
    - path: /var/mnt/vault/.config/homeassistant/blueprints/automation
      mode: 0o775
      user:
        name: home
      group:
        name: aivault
    - path: /var/mnt/vault/.config/zigbee2mqtt
      mode: 0o755
      user:
        name: home
      group:
        name: home
    - path: /var/mnt/vault/.config/mosquitto/config
      mode: 0o755
      user:
        name: home
      group:
        name: home
    - path: /var/mnt/vault/.config/mosquitto/log
      mode: 0o755
      user:
        name: home
      group:
        name: home
    - path: /var/mnt/vault/.config/mosquitto/data
      mode: 0o755
      user:
        name: home
      group:
        name: home
    - path: /home/home/.config
    - path: /home/home/.config/systemd
    - path: /home/home/.config/systemd/user
    - path: /home/home/.config/systemd/user/default.target.wants
